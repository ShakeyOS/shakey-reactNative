    "devDependencies": {
        "@types/jest": "^29.5.14",
        "@types/node": "^22.10.10",
        "jest": "^29.7.0",
        "ts-jest": "^29.2.5",
        "ts-node": "10.9.2",
        "tsup": "8.3.5",
        "typescript": "^5.7.3"
